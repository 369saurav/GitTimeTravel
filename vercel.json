{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/time-travel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/time-travel",
      "dest": "/api/time-travel.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
